<%= render 'users/userinfo', user: @user %>
<%= render 'books/new', book: @book %>
<table>
  <tr>
    <th>Title</th>
    <th>Opinion</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><p><%= book.title %></p></td>
      <td><p><%= book.body %></p></td>
    </tr>
  <% end %>
</table>
<%= paginate @books %>

